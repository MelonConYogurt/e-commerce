{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 120,
      "minLength": 1
    },
    "price": {
      "type": "biginteger",
      "required": true,
      "min": "0"
    },
    "stocksize10": {
      "type": "biginteger",
      "required": true,
      "min": "0",
      "default": "0"
    },
    "introduction": {
      "type": "string",
      "required": true,
      "maxLength": 250
    },
    "description": {
      "type": "text",
      "required": true
    },
    "media": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "colors": {
      "type": "json"
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "Hombre",
        "Mujer"
      ],
      "default": "Hombre",
      "required": true
    },
    "stocksize9": {
      "type": "biginteger",
      "default": "0"
    },
    "stocksize8": {
      "type": "biginteger",
      "default": "0"
    },
    "stocksize7": {
      "type": "biginteger",
      "default": "0"
    },
    "company": {
      "type": "enumeration",
      "enum": [
        "Nike",
        "Adidas "
      ],
      "default": "Nike"
    }
  }
}
